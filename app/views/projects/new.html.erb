<h1 class="for_form"> New Project </h1>      
<% form_for @project do |form| %>
  <%= hidden_field_tag 'project[user_id]', current_user.id %>
  <%= error_messages_for 'project' %>

  <%= form.label :name, "Project Name" %>
  <%= form.text_field :name %>

  <%= form.label :repository_url, "Repository URL" %>
  <%= form.text_field :repository_url %>

  <%= form.label :identifier, "Identifier" %>
  <%= form.text_field :identifier %>

  <%= form.submit "Register Project", :class => "submit" %>
<% end %>

<%= button_to "Back", :back, :method => :get %>


<%= javascript_tag "$('project_name').focus();" %>

